generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id         BigInt   @id @default(autoincrement())
  email      String?  @db.VarChar(255)
  username   String?  @db.VarChar(255)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
